{
    "name": "laravolt/ui",
    "description": "Admin template based on Laravel and Fomantic UI",
    "keywords": [
        "laravel",
        "laravolt",
        "admin",
        "template",
        "ui"
    ],
    "homepage": "https://github.com/laravolt/ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "bayu.hendra@javan.co.id",
            "homepage": "https://laravolt.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*",
        "illuminate/console": "5.6.*|5.7.*|5.8.*|6.*",
        "lavary/laravel-menu": "^1.7.2",
        "stolz/assets": "^0.1.11"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Ui\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Ui\\ServiceProvider",
                "Stolz\\Assets\\Laravel\\ServiceProvider"
            ]
        }
    }
}
